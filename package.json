{
  "name": "electron-react-parcel-three-template",
  "version": "0.1.0",
  "description": "A basic template for building apps with React.js, Electron, Parcel Bundler and Three.js",
  "author": "Daniel Fernandes",
  "homepage": "https://github.com/Carnaux/react-electron-parcel-three",
  "repository": {
    "type": "git",
    "url": "https://github.com/Carnaux/react-electron-parcel-three.git"
  },
  "bugs": {
    "url": "https://github.com/Carnaux/react-electron-parcel-three/issues"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "client": "parcel src/index.html --no-cache --out-dir=src/dist",
    "build": "parcel build src/index.html --no-cache --public-url ./ --out-dir=src/dist",
    "electron": "electron .",
    "dev": "concurrently \"npm run client\" \"npm run electron\"",
    "prod": "NODE_ENV=production electron ."
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.6.1",
    "electron": "^2.0.7",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "keywords": [
    "electron",
    "react",
    "parcel",
    "three"
  ],
  "dependencies": {
    "parcel-bundler": "^1.10.3",
    "three": "^0.98.0"
  }
}
